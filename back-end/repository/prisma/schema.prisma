generator client {
    provider = "prisma-client-js"
}
datasource db{
    provider = "postgresql"
    url = env("DATABASE_URL")
}
enum ActionType{
    attack
    movement
    spell
    legendary
    special
}
model Action{
    id          Int     @id @default(autoincrement())
    type        ActionType
    name        String
    description String
}